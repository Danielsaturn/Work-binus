from math import sqrt

print ("""This is a calculator for Quadratic Formula
""")

valueA = float(input("Please input the value of A: "))
valueB = float(input("Please input the value of B: "))
valueC = float(input("Please input the value of C: "))

disc = valueB**2 - 4 * valueA * valueC
squarerore = sqrt(abs(disc))

if (disc < 0):
    print ("The problem was", str(valueA), "xÂ²", str(valueB), "x", str(valueC), "and the solution is IMAGINARY")
